<div class="modal" id="myModali">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true"><span class="glyphicon glyphicon-remove"></span></span>
                </button>
                <span class="ion ion-person-add" style="float: left; font-size: 150%; margin: 5px 10px 0px 0px;"></span>
                <h4 style="float:left">Signup</h4>
            </div>
            <div class="modal-body">

                <form id="registrationForm" method="post" class="form-horizontal" action="register.php">
                    <div class="form-group ">
                        <label class="col-md-4 control-label" for="email">Email</label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="email" class="form-control" id="email" name="email" placeholder="Email..."><i
                                    class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">Email required</p>
                            <p class="errorMes display">Please enter a valid email address</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-4 control-label" for="pass">Password</label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="password" class="form-control" id="pass" name="pass" placeholder="Password..."><i
                                    class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">Password required</p>
                            <p class="errorMes display">Password must be more than 8 characters long</p>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="col-md-4 control-label" for="confirm">Confrim Password</label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="password" class="form-control" id="confirm" name="confirm"
                                   placeholder="Retype Password..."><i
                                    class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">Confirm password required</p>
                            <p class="errorMes display">Password does not match required</p>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-md-4" for="name1">Name</label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="text" class="form-control" id="name1" name="name1" placeholder="Name..."><i
                                    class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">Name required</p>
                            <p class="errorMes display">Name must not contain spaces and numbers</p>
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-md-4" for="last">Surname</label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="text" class="form-control" id="last" name="last" placeholder="Surname..."><i
                                    class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">Surname required</p>
                            <p class="errorMes display">Surname must not contain spaces and numbers</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4" for="age">Age</label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="text" class="form-control" id="age" name="age" placeholder="DD/MM/YYYY"><i
                                    class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">DOB required</p>
                            <p class="errorMes display">It must be in the correct format</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4" for="language">Language</label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="text" class="form-control" id="language" name="language"
                                   placeholder="Language..."><i class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">Language required</p>
                            <p class="errorMes display">Language must not contain numbers</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-md-4" id="captchaOperation"></label>
                        <div class="col-md-7 col-md-offset-1">
                            <input type="text" class="form-control" id="answer" name="captcha" placeholder=""> <i
                                    class="glyphicon glyphicon-remove test baste"></i><i
                                    class="glyphicon glyphicon-ok ok baste"></i>
                            <p class="errorMes display">Wrong answer</p>
                        </div>
                    </div>


                    <div class="form-group">
                        <label class="col-md-4 control-label">Gender</label>
                        <div class="col-md-7 col-md-offset-1">
                            <div class="radio">
                                <label>
                                    <input type="radio" id="male" name="gender" value="male"/> Male
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <input type="radio" id="female" name="gender" value="female"/> Female
                                </label>
                            </div>
                            <div class="radio">
                                <label>
                                    <input type="radio" id="other" name="gender" value="other"/> Other
                                </label>
                            </div>
                            <p class="errorMes display">Gender required</p>
                        </div>
                    </div>


